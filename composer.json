{
    "name": "rodrigorm/phpqa-patch",
    "description": "**phpqa** is a command-line tool for PHPMD, PHPCS and PHPCPD.",
    "type": "library",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Rodrigo Moyle",
            "email": "rodrigorm@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*"
    },
    "autoload": {
        "psr-4": {
            "Rodrigorm\\PhpqaPatch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rodrigorm\\PhpqaPatch\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": "phpunit"
    },
    "bin": [
        "phpqa"
    ]
}
